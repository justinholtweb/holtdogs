{
  "require": {
    "craftcms/cms": "4.2.5.1",
    "craftcms/redactor": "3.0.2",
    "internetztube/craft-element-relations": "2.0.2",
    "nystudio107/craft-eagerbeaver": "4.0.2",
    "ryssbowh/craft-restrict-deletions": "2.0.1",
    "spicyweb/craft-neo": "3.3.9",
    "verbb/super-table": "3.0.1",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
